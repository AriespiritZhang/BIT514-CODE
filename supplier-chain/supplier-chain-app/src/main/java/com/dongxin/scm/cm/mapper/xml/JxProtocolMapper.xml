<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dongxin.scm.cm.mapper.JxProtocolMapper">

    <select id="queryTitle" resultType="com.dongxin.scm.cm.entity.JxProtocolTitle">
        select str, level
        from jx_protocol_level;
    </select>

    <select id="queryAllCompany" resultType="java.lang.String">
        select name
        from yrm_company;
    </select>
</mapper>
